<?xml version="1.0" encoding="utf-8"?>
<!-- <!DOCTYPE configuration SYSTEM "rsm-config.dtd"> -->
<configuration>
	<general>
		<workspace>${squale.home}/Squalix/data/rsm</workspace>
		<reportpath>${squale.home}/Squalix/data/rsm/rapports_RSM.csv</reportpath>
		<reportmethodspath>${squale.home}/Squalix/data/rsm/rapports_Methods_RSM.csv</reportmethodspath>
		<reportclassespath>${squale.home}/Squalix/data/rsm/rapports_Class_RSM.csv</reportclassespath>
		<inputfile>${squale.home}/Squalix/data/rsm/input.txt</inputfile>
		<auxfile>${squale.home}/Squalix/data/rsm/rapport_aux.csv</auxfile>
		<execcommand>${ENV-RSM-EXEC}</execcommand>
	</general>
	<profiles>
		 <profile name="java">
	      	<extensions>.java</extensions>
	      	<entetes></entetes>
	      	<parameters>
	      		<!-- CSV output mode -->
	        	<parameter>-C</parameter>
	        	<!-- Pour afficher le nom de fichier input -->
	        	<parameter>-fa</parameter>
	        	<!-- les options pour récupérer le type de données voulues -->
	        	<parameter>-Tf</parameter>
	        	<!-- Pour avoir les métriques orientés objets -->
	        	<parameter>-To</parameter>
	        	<!-- Pour prendre une liste de fichiers en paramètres -->
	        	<parameter>-F</parameter>
	        	<!-- Fichier de sortie, sinon s'affiche à l'écran -->
	        	<parameter>-O</parameter>
	      	</parameters>
	     </profile>
		 <profile name="java compiled">
	      	<extensions>.java</extensions>
	      	<entetes></entetes>
	      	<parameters>
	      		<!-- CSV output mode -->
	        	<parameter>-C</parameter>
	        	<!-- Pour afficher le nom de fichier input -->
	        	<parameter>-fa</parameter>
	        	<!-- les options pour récupérer le type de données voulues -->
	        	<parameter>-Tf</parameter>
	        	<!-- Pour avoir les métriques orientés objets -->
	        	<parameter>-To</parameter>
	        	<!-- Pour prendre une liste de fichiers en paramètres -->
	        	<parameter>-F</parameter>
	        	<!-- Fichier de sortie, sinon s'affiche à l'écran -->
	        	<parameter>-O</parameter>
	      	</parameters>
	     </profile>
	     <profile name="j2ee">
	      	<extensions>.java</extensions>
	      	<entetes></entetes>
	      	<parameters>
	      		<!-- CSV output mode -->
	        	<parameter>-C</parameter>
	        	<!-- Pour afficher le nom de fichier input -->
	        	<parameter>-fa</parameter>
	        	<!-- les options pour récupérer le type de données voulues -->
	        	<parameter>-Tf</parameter>
	        	<!-- Pour avoir les métriques orientés objets -->
	        	<parameter>-To</parameter>
	        	<!-- Pour prendre une liste de fichiers en paramètres -->
	        	<parameter>-F</parameter>
	        	<!-- Fichier de sortie, sinon s'affiche à l'écran -->
	        	<parameter>-O</parameter>
	      	</parameters>
	     </profile>
	     <profile name="j2ee compiled">
	      	<extensions>.java</extensions>
	      	<entetes></entetes>
	      	<parameters>
	      		<!-- CSV output mode -->
	        	<parameter>-C</parameter>
	        	<!-- Pour afficher le nom de fichier input -->
	        	<parameter>-fa</parameter>
	        	<!-- les options pour récupérer le type de données voulues -->
	        	<parameter>-Tf</parameter>
	        	<!-- Pour avoir les métriques orientés objets -->
	        	<parameter>-To</parameter>
	        	<!-- Pour prendre une liste de fichiers en paramètres -->
	        	<parameter>-F</parameter>
	        	<!-- Fichier de sortie, sinon s'affiche à l'écran -->
	        	<parameter>-O</parameter>
	      	</parameters>
	     </profile>
	     <profile name="cpp">
	     	<extensions>.c,.cc,.cxx,.cpp,.C</extensions>
	      	<entetes>.h,.hh,.hxx,.hpp</entetes>
	     	<parameters>
		      	<!-- CSV output mode -->
	        	<parameter>-C</parameter>
	        	<!-- Pour afficher le nom de fichier input -->
	        	<parameter>-fa</parameter>
	        	<!-- les options pour récupérer le type de données voulues -->
	        	<parameter>-Tf</parameter>
	        	<!-- Pour avoir les métriques orientés objets -->
	        	<parameter>-To</parameter>
	        	<!-- Pour prendre une liste de fichiers en paramètres -->
	        	<parameter>-F</parameter>
	        	<!-- Fichier de sortie, sinon s'affiche à l'écran -->
	        	<parameter>-O</parameter>
	      	</parameters>
	     </profile>
    </profiles>
</configuration>