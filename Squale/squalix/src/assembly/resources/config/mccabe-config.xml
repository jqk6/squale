<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <general>
    <commands>
<!-- ENV-MCCABE-CLI -->
      <clicommand>${squale.home}/Squalix/bin/resources/mccabe_cli.sh</clicommand>
      <parsercommand>parse</parsercommand>
      <metricscommand>metrics -report %report_name% -ss -output %report_out%</metricscommand>
    </commands>
<!-- ENV-MCCABE-WORKSPACE -->
    <workspace>${squale.home}/Squalix/data/mccabe</workspace>
    <reposdir>${ENV-MCCABE-REPOSDIR}</reposdir>
<!-- ENV-MCCABE-LOG -->
    <logger>${squale.home}/Squalix/data/mccabe/logs/mccabe.log</logger>
<!-- ENV-MCCABE-LOG-ERROR -->
    <logger-err>${squale.home}/Squalix/data/mccabe/logs/mccabe_err.log</logger-err>
<!-- ENV-MCCABE-RPT -->
    <reportspath>${squale.home}/Squalix/bin/config/rapports.RPT</reportspath>
    <filter>
    	<errorToWarningMessages>
    		<message>ERR-gra2sc: 3012\.1: Completed build of class chart with warnings\. See the root/server window for details.*</message>
    		<message>WARN-metmng: 1099\.303: Licensing Error: 'Connection to license manager re-established\.'.*</message>
    		<message>.*WARN-gra2sc: 12018\.1:.*</message>
    	</errorToWarningMessages>
    	<ignoringMessages>
    		<message>ERR-cli: 1053\.117: Can't create directory '/OUTILS/McCabe/current/repos'.*</message>
			<message>ERR-cli: 1053\.117: Can't create directory '/exploit/OUTILS/McCabe/current/repos'.*</message>
    		<message>CRIT-cli: 1085\.304.*</message>
    	</ignoringMessages>
    	<addFileNameMessages>
    		<message>WARN-javapar: 2009\.302: parse error:.*</message>
    		<message>WARN-javapar: 1001\.325: Can't read class file.*</message>
    	</addFileNameMessages>
    	<replacingMessages>
    		<replacingMessage 
    			msg="McCabe's execution environment isn't correct, contact your SQUALE administrator.*" 
    			replace="McCabe's execution environment isn't correct, contact your SQUALE administrator."/>
    		<replacingMessage msg="WARN-cli: 1099\.309.*" 
    			replace="No licence for McCabe execution. A new test will be done later."/>
    	</replacingMessages>
    </filter>
  </general>
