<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.airfrance.squalecommon.enterpriselayer.businessobject.stats.SiteStatsDICTBO"
        table="Stats_squale_dict"
        mutable="true"
    >

        <id
            name="id"
            column="Id"
            type="long"
            length="19"
            unsaved-value="-1"
        >
            <generator class="native">
                <param name="sequence">stats_sequence</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-SiteStatsDICTBO.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="nbOfAcceptedFactors"
            type="integer"
            update="true"
            insert="true"
            column="NB_FACTEURS_ACCEPTES"
            not-null="true"
        />

        <property
            name="nbOfAcceptedWithReservesFactors"
            type="integer"
            update="true"
            insert="true"
            column="NB_FACTEURS_ACCEPTES_RESERVES"
            not-null="true"
        />

        <property
            name="nbOfRefusedFactors"
            type="integer"
            update="true"
            insert="true"
            column="NB_FACTEURS_REFUSES"
            not-null="true"
        />

        <property
            name="nbTotalAppli"
            type="integer"
            update="true"
            insert="true"
            column="NB_TOTAL_APPLI"
            not-null="true"
        />

        <property
            name="nbTotalAppliWithAudit"
            type="integer"
            update="true"
            insert="true"
            column="NB_APPLI_AVEC_AUDIT_EXECUTE"
            not-null="true"
        />

        <property
            name="nbTotalAppliWithSuccesfulAudit"
            type="integer"
            update="true"
            insert="true"
            column="NB_APPLI_AVEC_AUDIT_REUSSI"
            not-null="true"
        />

        <property
            name="today"
            type="timestamp"
            update="true"
            insert="true"
            column="Date_calcul"
            not-null="true"
        />

        <property
            name="roi"
            type="double"
            update="true"
            insert="true"
            column="ROI_EN_MHI"
            not-null="true"
        />

        <property
            name="nbAppliToValidate"
            type="integer"
            update="true"
            insert="true"
            column="NB_APPLI_A_VALIDER"
            not-null="true"
        />

        <property
            name="nbFailedAudits"
            type="integer"
            update="true"
            insert="true"
            column="NB_AUDITS_ECHECS"
            not-null="true"
        />

        <property
            name="nbNotAttemptedAudit"
            type="integer"
            update="true"
            insert="true"
            column="NB_AUDITS_PROGRAMME"
            not-null="true"
        />

        <property
            name="nbPartialAudits"
            type="integer"
            update="true"
            insert="true"
            column="NB_AUDITS_PARTIELS"
            not-null="true"
        />

        <property
            name="nbSuccessfulAudits"
            type="integer"
            update="true"
            insert="true"
            column="NB_AUDITS_REUSSIS"
            not-null="true"
        />

        <property
            name="nbTotalAppliWithoutAudit"
            type="integer"
            update="true"
            insert="true"
            column="NB_APPLI_AVEC_AUCUN_AUDIT"
            not-null="true"
        />

        <many-to-one
            name="serveurBO"
            class="com.airfrance.squalecommon.enterpriselayer.businessobject.config.ServeurBO"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="Serveur"
            not-null="false"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-SiteStatsDICTBO.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
