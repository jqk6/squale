<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class lazy="true" table="TaskRef" name="com.airfrance.squalecommon.enterpriselayer.businessobject.config.TaskRefBO">
    <id unsaved-value="-1" name="id" type="long" column="TaskRefId" length="19">
      <generator class="native">
        <param name="sequence">taskRef_sequence</param>
      </generator>
    </id>
    <bag cascade="all" lazy="true" table="TaskParameter" name="parameters">
      <key column="TaskRefId"/>
      <one-to-many class="com.airfrance.squalecommon.enterpriselayer.businessobject.config.TaskParameterBO"/>
    </bag>
    <many-to-one insert="true" column="TaskId" cascade="all" update="true" outer-join="auto" class="com.airfrance.squalecommon.enterpriselayer.businessobject.config.TaskBO" name="task"/>
  </class>
</hibernate-mapping>
