<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.airfrance.squalecommon.enterpriselayer.businessobject.result.QualityResultBO"
        table="QualityResult"
        mutable="true"
        discriminator-value="QualityResult"
    >

        <id
            name="id"
            column="QualityResultId"
            type="long"
            length="19"
            unsaved-value="-1"
        >
            <generator class="native">
                <param name="sequence">qualityres_sequence</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-QualityResultBO.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <discriminator
            column="subclass"
        />

        <many-to-one
            name="rule"
            class="com.airfrance.squalecommon.enterpriselayer.businessobject.rule.QualityRuleBO"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="QualityRuleId"
            not-null="true"
        />

        <property
            name="meanMark"
            type="float"
            update="true"
            insert="true"
            column="MeanMark"
            not-null="true"
            unique="false"
        />

        <many-to-one
            name="project"
            class="com.airfrance.squalecommon.enterpriselayer.businessobject.component.ProjectBO"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="ProjectId"
            not-null="true"
        />

        <many-to-one
            name="audit"
            class="com.airfrance.squalecommon.enterpriselayer.businessobject.component.AuditBO"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="AuditId"
            not-null="true"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-QualityResultBO.xml
            containing the additional properties and place it in your merge dir.
        -->
        <subclass
            name="com.airfrance.squalecommon.enterpriselayer.businessobject.result.PracticeResultBO"
            discriminator-value="PracticeResult"
        >

        <array
            name="floatRepartition"
            table="PracticeResult_Repartition"
            cascade="none"
        >

            <key
                column="PracticeResultId"
            >
            </key>

            <index
                column="Repartition"
                type="long"
                length="19"
            />

            <element
                column="Repartition_value"
                type="java.lang.Integer"
                length="10"
                not-null="false"
                unique="false"
            />

        </array>

	    <!--
            	To add non XDoclet property mappings, create a file named
                hibernate-properties-PracticeResultBO.xml
		containing the additional properties and place it in your merge dir.
	    -->

        </subclass>
        <subclass
            name="com.airfrance.squalecommon.enterpriselayer.businessobject.result.CriteriumResultBO"
            discriminator-value="CriteriumResult"
        >
	    <!--
            	To add non XDoclet property mappings, create a file named
                hibernate-properties-CriteriumResultBO.xml
		containing the additional properties and place it in your merge dir.
	    -->

        </subclass>
        <subclass
            name="com.airfrance.squalecommon.enterpriselayer.businessobject.result.FactorResultBO"
            discriminator-value="FactorResult"
        >
	    <!--
            	To add non XDoclet property mappings, create a file named
                hibernate-properties-FactorResultBO.xml
		containing the additional properties and place it in your merge dir.
	    -->

        </subclass>

    </class>

</hibernate-mapping>
