    <many-to-one insert="true" column="AuditId" cascade="none" update="true" outer-join="auto" class="com.airfrance.squalecommon.enterpriselayer.businessobject.component.AuditBO" not-null="true" name="audit"/>
    <subclass name="com.airfrance.squalecommon.enterpriselayer.businessobject.result.CriteriumResultBO" discriminator-value="CriteriumResult"/>
    <subclass name="com.airfrance.squalecommon.enterpriselayer.businessobject.result.FactorResultBO" discriminator-value="FactorResult"/>
