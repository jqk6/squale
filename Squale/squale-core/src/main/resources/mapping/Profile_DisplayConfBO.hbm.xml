<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.airfrance.squalecommon.enterpriselayer.businessobject.config.Profile_DisplayConfBO"
        table="Profile_DisplayConfBO"
        lazy="true"
    >

        <id
            name="id"
            column="ConfId"
            type="long"
            length="19"
            unsaved-value="-1"
        >
            <generator class="native">
                <param name="sequence">profiledisplayconf_sequence</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-Profile_DisplayConfBO.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <many-to-one
            name="displayConf"
            class="com.airfrance.squalecommon.enterpriselayer.businessobject.config.web.AbstractDisplayConfBO"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="Profile_ConfId"
            not-null="false"
        />

        <many-to-one
            name="profile"
            class="com.airfrance.squalecommon.enterpriselayer.businessobject.config.ProjectProfileBO"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="ProfileId"
            not-null="false"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Profile_DisplayConfBO.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
