<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.airfrance.squalecommon.enterpriselayer.businessobject.stats.SiteAndProfilStatsDICTBO"
        table="Stats_squale_dict_annexe"
    >

        <id
            name="id"
            column="Id"
            type="long"
            length="19"
            unsaved-value="-1"
        >
            <generator class="native">
                <param name="sequence">stats_annexe_sequence</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-SiteAndProfilStatsDICTBO.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="nbOfCodesLines"
            type="integer"
            update="true"
            insert="true"
            column="NB_LIGNES"
            not-null="true"
        />

        <property
            name="profil"
            type="string"
            update="true"
            insert="true"
            column="Profil"
            length="4000"
            not-null="true"
        />

        <property
            name="nbProjects"
            type="integer"
            update="true"
            insert="true"
            column="NB_PROJETS"
            not-null="true"
        />

        <many-to-one
            name="serveurBO"
            class="com.airfrance.squalecommon.enterpriselayer.businessobject.config.ServeurBO"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="Serveur"
            not-null="false"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-SiteAndProfilStatsDICTBO.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
