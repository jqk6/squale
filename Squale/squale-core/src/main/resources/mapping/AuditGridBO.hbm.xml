<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.airfrance.squalecommon.enterpriselayer.businessobject.component.AuditGridBO"
        table="AuditGridBO"
        mutable="true"
    >

        <id
            name="id"
            column="AuditGridId"
            type="long"
            length="19"
            unsaved-value="-1"
        >
            <generator class="native">
                <param name="sequence">auditgrid_sequence</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-AuditGridBO.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <many-to-one
            name="grid"
            class="com.airfrance.squalecommon.enterpriselayer.businessobject.rule.QualityGridBO"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="QualityGridId"
            not-null="false"
        />

        <many-to-one
            name="project"
            class="com.airfrance.squalecommon.enterpriselayer.businessobject.component.ProjectBO"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="ProjectId"
            not-null="false"
        />

        <many-to-one
            name="audit"
            class="com.airfrance.squalecommon.enterpriselayer.businessobject.component.AuditBO"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="AuditId"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-AuditGridBO.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
