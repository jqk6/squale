<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.airfrance.squalecommon.enterpriselayer.businessobject.rulechecking.checkstyle.CheckstyleModuleBO"
        table="Module"
        mutable="true"
    >

        <id
            name="id"
            column="ModuleId"
            type="long"
            length="19"
            unsaved-value="-1"
        >
            <generator class="native">
                <param name="sequence">Module_sequence</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-CheckstyleModuleBO.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="message"
            type="string"
            update="true"
            insert="true"
            column="Message"
            not-null="false"
            unique="false"
        />

        <property
            name="name"
            type="string"
            update="true"
            insert="true"
            column="Name"
            not-null="false"
            unique="false"
        />

        <many-to-one
            name="rule"
            class="com.airfrance.squalecommon.enterpriselayer.businessobject.rulechecking.checkstyle.CheckstyleRuleBO"
            cascade="save-update"
            outer-join="auto"
            update="true"
            insert="true"
            column="RuleId"
            not-null="true"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-CheckstyleModuleBO.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
